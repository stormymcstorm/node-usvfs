{
  "name": "@stormymcstorm/node_usvfs",
  "version": "0.0.1",
  "description": "Node.js bindings for USVFS",
  "main": "./bindings.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "os": [
    "win32"
  ],
	"cpu": [
		"x64"
	],
  "scripts": {
    "install": "node-pre-gyp install",
    "build": "node-pre-gyp rebuild",
    "build:boost": "cd deps/boost && bootstrap && b2 --with-date_time --with-coroutine --with-filesystem --with-thread --with-log --with-locale address-model=64 architecture=x86 link=static runtime-link=static",
    "build:udist86-optable": "cd deps/usvfs/udis86 && python scripts/ud_itab.py docs/x86/optable.xml libudis86",
    "dev-setup": "node-pre-gyp configure && npm run build:boost && npm run build:udist86-optable && npm run rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stormymcstorm/node_usvfs.git"
  },
  "author": "Carson Storm <stormymcstorm@gmail.com>",
  "contributors": [
    {
      "name": "Jordan Tucker",
      "email": "jordanbtucker@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stormymcstorm/node_usvfs/issues"
  },
  "homepage": "https://github.com/stormymcstorm/node_usvfs#readme",
  "binary": {
    "module_name": "node_usvfs",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/stormymcstorm/node_usvfs/releases/download/",
    "remote_path": "v{version}"
  },
  "dependencies": {
    "node-pre-gyp": "^0.11.0"
  },
  "devDependencies": {
		"napi-thread-safe-callback": "^0.0.6",
    "node-addon-api": "^1.4.0",
    "node-gyp": "^3.8.0",
    "tap": "^12.0.1"
  }
}
